    "\n",
    "class PressureSolver:\n",
    "    '''\n",
    "    A finite difference solver to solve pressure distribution in\n",
    "    a reservoir, logarithmic grid has been used, y = ln(r/rw)\n",
    "    The solver uses SI units internally, while \"practical field units\"\n",
    "    are required as input.\n",
    "    Input arguments:\n",
    "    name                                symbol                  unit\n",
    "    --------------------------------------------------------------\n",
    "    Number of grid points               N                       dimensionless\n",
    "    Constant time step                  dt                      days\n",
    "    Well radius                         rw                      ft\n",
    "    Outer reservoir boundary            re                      ft\n",
    "    Height of reservoir                 h                       ft\n",
    "    Absolute permeability               k                       mD\n",
    "    Porosity                            phi                     dimensionless\n",
    "    Fluid viscosity                     mu                      mPas (cP)\n",
    "    Total (rock+fluid) compressibility  ct                      1 / psi\n",
    "    Constant flow rate at well          Q                       bbl / day\n",
    "    Initial reservoir pressure          pi                      psi\n",
    "    -------------------------------------------------------------\n",
    "    '''\n",
    "    \n",
    "    def __init__(self, N, lazy=True, sparse= False, dt=1, rw=0.318, re=1000.0, h=11.0, phi=0.25, mu=1.0,\n",
    "    ct=7.8e-6, Q=1000.0, k=500, pi=4100.0, pb=8200):\n",
    "     #Unit conversion factors (input units --> SI)\n",
    "        self.ft_to_m_ = 0.3048\n",
    "        self.psi_to_pa_ = 6894.75729\n",
    "        self.day_to_sec_ = 24.*60.*60.\n",
    "        self.bbl_to_m3_ = 0.1589873\n",
    "\n",
    "        #Rock and fluid properties\n",
    "        self.L=1 # m\n",
    "        self.k = k*1e-15 / 1.01325 # from Darcy to m^2\n",
    "        self.sigma = k*1e-15 / 1.01325 # from Darcy to m^2\n",
    "        #self.sigma = 1\n",
    "        #self.beta=self.sigma*self.L**2/self.k\n",
    "        self.beta=1\n",
    "\n",
    "\n",
    "        self.phi_ = phi\n",
    "        self.mu_ = mu*1e-3 # from cP to Pas\n",
    "        self.ct_ = ct / self.psi_to_pa_\n",
    "        \n",
    "        \n",
    "        #Grid\n",
    "        self.N = N\n",
    "        self.dy=1/self.N\n",
    "        self.y=np.arange(self.dy/2,self.L,self.dy)\n",
    "        self.rw = rw*self.ft_to_m_\n",
    "        self.re = re*self.ft_to_m_\n",
    "        self.h = h*self.ft_to_m_\n",
    "    \n",
    "        \n",
    "    \n",
    "        #Initial and boundary conditions\n",
    "\n",
    "        self.Q_ = Q*self.bbl_to_m3_ / self.day_to_sec_\n",
    "        self.pi_ = pi*self.psi_to_pa_\n",
    "\n",
    "        self.pb=pb #boundary pressure\n",
    "        self.lazy=lazy # boundary condition\n",
    "        self.sparse=sparse # use sparse matrix?\n",
    "    \n",
    "        # Time control for simulation\n",
    "        self.dt_ = dt*self.day_to_sec_\n",
    "\n",
    "        self.current_time=0.\n",
    "        self.time=[]\n",
    "\n",
    "        self.A=np.array([]) # Holds the matrix to be solved\n",
    "        self.A_sparse=np.array([]) # Holds the matrix to be solved (sparse?)\n",
    "        self.P=np.array([]) # current pressure\n",
    "        self.rhs=np.array([]) # right hand side of the linear system\n",
    "        self.pi=np.repeat(self.pb, self.N)\n",
    "    \n",
    "    # TO DO: Add more stuff below here....\n",
    "    # (grid coordinates, dy, eta, etc.)\n",
    "    # \n",
    "    \n",
    "    def set_up_matrix_and_bc(self):\n",
    "        a=np.array(np.ones(self.N-1))\n",
    "        b=np.array(np.repeat(-2,self.N))\n",
    "        c=np.array(np.ones(self.N-1))\n",
    "        \n",
    "        b[0]=-1\n",
    "        if not self.lazy:\n",
    "            b[-1]=-2\n",
    "        self.A=np.diag(a,k=-1)+np.diag(b,k=0)+np.diag(c,k=1)\n",
    "        \n",
    "    \n",
    "    def set_up_rhs_and_bc(self):\n",
    "        self.rhs=-self.beta*self.dy**2*np.array(np.ones(self.N))\n",
    "        if self.lazy:\n",
    "            self.rhs[-1]=self.rhs[-1]-self.pb\n",
    "        else: # better boundary condition\n",
    "            self.rhs[-1]=self.rhs[-1]-2*self.pb\n",
    "        \n",
    "    def solve(self):\n",
    "        self.solver_called=True\n",
    "        self.set_up_matrix_and_bc()\n",
    "        self.set_up_rhs_and_bc()\n",
    "        self.T=np.linalg.solve(self.A,self.rhs)\n",
    "    \n",
    "    def solve_timedependent(self):\n",
    "        self.solver_called=True\n",
    "        self.set_up_matrix_for_time_dependent()\n",
    "        Pold=self.Pinit\n",
    "        self.Pt=Pold\n",
    "        self.time.append(self.current_time)\n",
    "\n",
    "        while(self.current_time <= self.Pfinal):\n",
    "            rhs=self.set_up_rhs_and_bc_timedependent(Pold)\n",
    "            if not self.sparse:\n",
    "                Pnew=np.linalg.solve(self.A,rhs)\n",
    "            else:\n",
    "                Pnew=scipy.sparse.linalg.spsolve(self.A_sparse,rhs)\n",
    "            self.Pt=np.append(self.Pt,Pnew)\n",
    "            Pold=Pnew\n",
    "            self.current_time = self.current_time + self.dt\n",
    "            self.time.append(self.current_time)\n",
    "            self.plot(Tnew)\n"
   ]
